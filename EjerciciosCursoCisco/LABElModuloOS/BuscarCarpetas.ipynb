    "import os\n",
    "\n",
    "path = 'tree'\n",
    "direc = 'python'\n",
    "\n",
    "def find_dirs(path, direc):\n",
    "    for i in os.listdir(path):\n",
    "        if i == direc:\n",
    "            yield path + '/' + i\n",
    "        yield from find_dirs(path + '/' + i, direc)\n",
    "\n",
    "for i in list(find_dirs(path, direc)): print(i)\n"
   ]
  }
